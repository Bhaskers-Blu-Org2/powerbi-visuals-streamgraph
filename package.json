{
  "name": "powerbi-visuals-streamgraph",
  "version": "0.5.0",
  "description": "A stacked area chart with smooth interpolation. Often used to display values over time.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/powerbi-visuals-streamgraph.git"
  },
  "keywords": [
    "powerbi-visuals"
  ],
  "scripts": {
    "postinstall": "typings install && pbiviz update 1.3.0",
    "typings": "typings",
    "pbiviz": "pbiviz",
    "start": "pbiviz start",
    "package": "pbiviz package",
    "lint": "node node_modules/tslint/bin/tslint \"+(src|test)/**/*.ts\"",
    "pretest": "pbiviz package --resources --no-minify --no-pbiviz",
    "test": "karma start"
  },
  "author": {
    "name": "Microsoft",
    "email": "pbicvsupport@microsoft.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Microsoft/powerbi-visuals-streamgraph/issues"
  },
  "homepage": "https://github.com/Microsoft/powerbi-visuals-streamgraph#readme",
  "devDependencies": {
    "powerbi-visuals-tools": "1.3.0",
    "tslint": "4.0.2",
    "typings": "2.0.0"
  }
}
